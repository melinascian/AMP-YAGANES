@article{McDonald-Madden2016,
  title = {Using Food-Web Theory to Conserve Ecosystems},
  author = {{McDonald-Madden}, E. and Sabbadin, R. and Game, E. T. and Baxter, P. W. J. and Chad{\`e}s, I. and Possingham, H. P.},
  year = {2016},
