@article{Chades2011,
  title = {General Rules for Managing and Surveying Networks of Pests, Diseases, and Endangered Species},
  author = {Chad{\`e}s, Iadine and Martin, Tara G. and Nicol, Samuel and Burgman, Mark A. and Possingham, Hugh P. and Buckley, Yvonne M.},
  year = {2011},
  month = may,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {108},
  number = {20},
  pages = {8323--8328},
  publisher = {Proceedings of the National Academy of Sciences},
  doi = {10.1073/pnas.1016846108},
  urldate = {2024-01-10},
  abstract = {o El manejo eficiente de enfermedades, plagas o especies en peligro de extinci{\'o}n es un problema global importante que enfrentan las agencias que cuentan con recursos limitados. El desaf{\'i}o de la gesti{\'o}n es a{\'u}n mayor cuando los organismos son dif{\'i}ciles de detectar. Mostramos c{\'o}mo priorizar los esfuerzos de gesti{\'o}n y encuestas en el tiempo y el espacio para redes de subpoblaciones susceptibles, infectadas y susceptibles. Presentamos reglas generales simples y s{\'o}lidas para proteger subpoblaciones deseables o erradicar las indeseables conectadas en patrones de red t{\'i}picos (motivos). Adem{\'a}s, demostramos que estas reglas se pueden generalizar a redes m{\'a}s grandes cuando los motivos se combinan en formaciones m{\'a}s complejas. Los resultados muestran que el mejor lugar para gestionar o estudiar una plaga o enfermedad en una red es tambi{\'e}n el mejor lugar para proteger o estudiar una especie en peligro de extinci{\'o}n. El punto de partida {\'o}ptimo en una red es el motivo m{\'a}s r{\'a}pido de gestionar, donde los motivos de l{\'i}nea, estrella, isla y c{\'u}mulo var{\'i}an de r{\'a}pido a lento. Gestionar el nodo m{\'a}s conectado en el momento adecuado y mantener la misma direcci{\'o}n de gesti{\'o}n proporciona ventajas sobre las estrategias externas-interiores recomendadas anteriormente. Cuando no se detecta una especie o enfermedad y nuestra creencia en la persistencia disminuye, nuestros resultados recomiendan desviar recursos hacia la gesti{\'o}n o vigilancia de los nodos m{\'a}s conectados. Nuestra aproximaci{\'o}n anal{\'i}tica proporciona orientaci{\'o}n sobre cu{\'a}nto tiempo debemos gestionar o estudiar las redes en busca de organismos dif{\'i}ciles de detectar. Nuestras reglas tienen en cuenta el {\'e}xito de la gesti{\'o}n, la dispersi{\'o}n, el costo econ{\'o}mico y la detecci{\'o}n imperfecta y ofrecen a los administradores una base pr{\'a}ctica para gestionar redes relevantes para muchas cuestiones ambientales, de bioseguridad y de salud humana importantes.},
  file = {/Users/tomasmarina/Zotero/storage/K5WFS5RF/Chadès et al. - 2011 - General rules for managing and surveying networks .pdf}
}

@incollection{Cleveland1992,
  title = {Local {{Regression Models}}},
  booktitle = {Statistical {{Models}} in {{S}}},
  author = {Cleveland, W. S. and Grosse, E. and Shyu, W. M.},
  year = {1992},
  pages = {68},
  publisher = {{Wadsworth and Brooks/Cole}},
  abstract = {Local regression models provide methods for fitting regression functions, or regression surfaces, to data. The chapter describes the specifications of a particular model determine the details of the method used to fit the model; the fitting method, which is called loess. It discusses the S functions and objects for local regression models by working through a number of examples. Our goal is to show how the data are analyzed in practice using S. This means we must discuss diagnostic methods. Thus, diagnostic checking is an essential part of the practice of fitting local regression models, and, as with all model building, omitting it results in demonstrated validity being replaced simply by hope. Thus, the fitting of local regression models involves making the following choices about the specification of properties of the errors and the regression surface. Properties are Gaussian or symmetric distribution; constant variance or a priori weights; locally linear or locally quadratic in numeric predictors; and neighborhood size.},
  isbn = {978-0-203-73853-5},
  langid = {english}
}

@article{Cohen1984,
  title = {Trophic Links of Community Food Webs.},
  author = {Cohen, J E and Briand, F},
  year = {1984},
  month = jul,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {81},
  number = {13},
  pages = {4105--4109},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.81.13.4105},
  urldate = {2024-01-10},
  abstract = {This report describes and explains regularities in the numbers and kinds of trophic links in community food webs. To a first approximation, the mean number of trophic links in a community food web is proportional to the total number of trophic species. The mean number of trophic links between any two categories of trophic species (basal, intermediate, and top) is proportional to the geometric mean number of species in the categories joined. These linear relationships, and scale-invariance in the proportions of basal, intermediate, and top species, make it possible to predict with remarkable precision the proportions of each kind of trophic link among all community food webs. The differences between food webs in constant and in fluctuating environments reflect apparently greater constraints on the trophic organization of food webs in fluctuating environments.},
  langid = {english},
  file = {/Users/tomasmarina/Zotero/storage/K9BR28ES/Cohen y Briand - 1984 - Trophic links of community food webs..pdf}
}

@article{Collins2008,
