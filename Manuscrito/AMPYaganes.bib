@article{Riccialdelli2010,
  title = {{Isotopic assessment of prey and habitat preferences of a cetacean community in the southwestern South Atlantic Ocean}},
  author = {Riccialdelli, Luciana and Newsome, Seth D. and Fogel, Marilyn L. and Goodall, R. Natalie P.},
  year = {2010},
  month = nov,
  journal = {Marine Ecology Progress Series},
  volume = {418},
  pages = {235--248},
  issn = {0171-8630, 1616-1599},
  doi = {10.3354/meps08826},
  urldate = {2022-12-13},
  abstract = {RESUMEN: Usamos an{\'a}lisis de is{\'o}topos estables de carbono ({$\delta$} 13 C) y nitr{\'o}geno ({$\delta$} 15 N) para investigar la ecolog{\'i}a tr{\'o}fica de 8 especies de peque{\~n}os cet{\'a}ceos del suroeste del Oc{\'e}ano Atl{\'a}ntico Sur: 6 delf{\'i}nidos ( Grampus griseus , Lagenorhynchus cruciger , L. australis , Lissodelphis peronii , Pseudorca crassidens y Cephalorhynchus commersonii ) y 2 focoenidos ( Phocoena dioptrica y P. spinipinnis ). Tambi{\'e}n analizamos muestras de posibles presas recolectadas de h{\'a}bitats oce{\'a}nicos y costeros adyacentes a Tierra del Fuego. Hueso de cet{\'a}ceo-col{\'a}geno {$\delta$} 13 C y {$\delta$} 15Los datos de N revelaron informaci{\'o}n tanto sobre el h{\'a}bitat como sobre las preferencias de presa. Observamos un continuo isot{\'o}pico en el que las especies costeras ten{\'i}an los valores m{\'a}s altos de {$\delta$} 13 C y {$\delta$} 15 N ( L. australis ), mientras que las especies oce{\'a}nicas y australes ten{\'i}an los valores m{\'a}s bajos ( L. cruciger y P. dioptrica ), indicativo de alimentaci{\'o}n en alta mar en aguas oce{\'a}nicas fr{\'i}as cerca de la Convergencia Ant{\'a}rtica. La superposici{\'o}n de los valores medios de is{\'o}topos entre C. commersonii y P. spinipinnis sugiere que estas especies pueden tener preferencias similares de h{\'a}bitat y/o presas. Resultados de is{\'o}topos para L. peronii , P. crassidens y G. griseussugieren que en estas latitudes ({\textasciitilde}54{$^\circ$}S) se alimentan en la plataforma continental exterior. G. griseus muestra patrones isot{\'o}picos bimodales, lo que sugiere que en esta regi{\'o}n del suroeste del Oc{\'e}ano Atl{\'a}ntico se encuentran dos ecotipos que se alimentan en diferentes h{\'a}bitats y/o consumen diferentes presas. Los datos isot{\'o}picos presentados aqu{\'i} brindan informaci{\'o}n sobre la ecolog{\'i}a de estos cet{\'a}ceos, con implicaciones relevantes para su manejo y conservaci{\'o}n exitosos.},
  langid = {spanish},
  keywords = {13C,15N,Food/prey,Foraging areas,Small cetaceans,South Atlantic Ocean,Southern Ocean,Stable isotopes},
  file = {/Users/tomasmarina/Zotero/storage/SZRKQRBV/Riccialdelli et al. - 2010 - Isotopic assessment of prey and habitat preference.pdf}
}

@article{Riccialdelli2017,
  title = {{Evaluating the isotopic niche of beaked whales from the southwestern South Atlantic and Southern Oceans}},
  author = {Riccialdelli, Luciana and Viola, M. Natalia Paso and Panarello, Hector O. and Goodall, R. Natalie P.},
  year = {2017},
  month = oct,
  journal = {Marine Ecology Progress Series},
  volume = {581},
  pages = {183--198},
  issn = {0171-8630, 1616-1599},
  doi = {10.3354/meps12296},
  urldate = {2022-12-13},
  abstract = {RESUMEN: El estudio de los nichos de las especies se est{\'a} volviendo cada vez m{\'a}s importante en la investigaci{\'o}n ecol{\'o}gica con la creciente necesidad de comprender c{\'o}mo las especies y su h{\'a}bitat responden a los r{\'a}pidos cambios ambientales. Este tipo de estudio es especialmente desafiante en el caso de especies oce{\'a}nicas y cr{\'i}pticas como las ballenas picudas. Analizamos los valores de {$\delta$} 13 C y {$\delta$} 15 N en col{\'a}geno {\'o}seo para estudiar la variaci{\'o}n interespec{\'i}fica en el nicho isot{\'o}pico de 8 especies de ballenas picudas ( Berardius arnuxii , Hyperoodon planifrons , Mesoplodon bowdoini , M. grayi , M. hectori , M. layardii , Tasmacetus Shepherdi y Ziphius cavirostris) que var{\'o} a lo largo de la costa de Tierra del Fuego, Argentina, entre 1967 y 2009. Encontramos una segregaci{\'o}n isot{\'o}pica espacial (p. ej., {\'a}reas de alimentaci{\'o}n) y tr{\'o}fica (p. ej., dieta) entre estas especies, que se sabe que habitan aguas subant{\'a}rticas a ant{\'a}rticas. El an{\'a}lisis isot{\'o}pico mostr{\'o} que, mientras que la segregaci{\'o}n espacial parece operar en un grado mayor en especies del mismo g{\'e}nero (por ejemplo, Mesoplodon ), la diferenciaci{\'o}n tr{\'o}fica permiti{\'o} la coexistencia de especies simp{\'a}tricas de diferentes g{\'e}neros (por ejemplo, Ziphius , Hyperodoon y Mesoplodon ).). Nuestros resultados concuerdan con estudios previos basados \hspace{0pt}\hspace{0pt}en an{\'a}lisis de contenido estomacal. Adem{\'a}s de las diferentes estrategias tr{\'o}ficas, el an{\'a}lisis de is{\'o}topos estables tambi{\'e}n revel{\'o} variaciones en las preferencias de h{\'a}bitat. Se encontr{\'o} una tendencia isot{\'o}pica en los valores de {$\delta$} 13 C y {$\delta$} 15 N entre las ballenas picudas y las posibles especies de presa de acuerdo con informaci{\'o}n previa sobre otras especies de cet{\'a}ceos que se sabe que tienen {\'a}reas de alimentaci{\'o}n en diferentes regiones oce{\'a}nicas. El presente estudio proporciona informaci{\'o}n ecol{\'o}gica valiosa sobre especies de ballenas picudas escurridizas y poco estudiadas que habitan en los oc{\'e}anos m{\'a}s australes del mundo.},
  langid = {spanish},
  keywords = {13C,15N,Polar waters,Subantarctic waters,Trophic niche,Ziphiidae},
  file = {/Users/tomasmarina/Zotero/storage/45B3PTV2/Riccialdelli et al. - 2017 - Evaluating the isotopic niche of beaked whales fro.pdf}
}

@article{Rosciano2016,
  title = {{Do penguins share? Evidence of foraging niche segregation between but not within two sympatric, central-place foragers}},
  shorttitle = {{Do penguins share?}},
  author = {Rosciano, N. G. and Polito, M. J. and Rey, A. Raya},
  year = {2016},
  month = apr,
  journal = {Marine Ecology Progress Series},
  volume = {548},
  pages = {249--262},
  issn = {0171-8630, 1616-1599},
  doi = {10.3354/meps11689},
  urldate = {2022-12-13},
  abstract = {RESUMEN: La teor{\'i}a del nicho predice que las especies simp{\'a}tricas deben diferenciarse ecol{\'o}gicamente para poder coexistir y las conespec{\'i}ficas tambi{\'e}n se diferenciar{\'a}n para reducir la competencia intraespec{\'i}fica. Como recolectores de lugares centrales, las aves marinas reproductoras coloniales representan un sistema modelo ideal para probar esta teor{\'i}a y examinar el mecanismo de segregaci{\'o}n de nichos. Utilizamos dispositivos GPS-TDlog para rastrear y bucear datos y an{\'a}lisis de is{\'o}topos estables para examinar patrones de segregaci{\'o}n de nichos interespec{\'i}ficos e intraespec{\'i}ficos entre ping{\"u}inos de penacho amarillo del sur Eudyptes chrysocome chrysocome y ping{\"u}inos de Magallanes Spheniscus magellanicusreproduci{\'e}ndose en la Isla de los Estados, Argentina, durante 3 temporadas reproductivas consecutivas. Los datos de seguimiento indicaron una fuerte segregaci{\'o}n espacial interespec{\'i}fica de las ubicaciones de alimentaci{\'o}n y poca superposici{\'o}n. Los datos de buceo tambi{\'e}n destacaron la segregaci{\'o}n del nicho de alimentaci{\'o}n vertical cuando las hembras de ping{\"u}ino penacho amarillo se sumergieron m{\'a}s profundo que los machos y hembras de ping{\"u}inos de Magallanes. Los valores de {$\delta$} 13 C respaldaron el patr{\'o}n general de segregaci{\'o}n del h{\'a}bitat, con valores m{\'a}s bajos para las hembras de ping{\"u}ino penacho amarillo que se sumergieron m{\'a}s profundo y buscaron alimento fuera de la plataforma. Las hembras de ping{\"u}ino penacho amarillo exhibieron un valor tr{\'o}fico relativo m{\'a}s bajo ({$\delta$} 15N) que los ping{\"u}inos de Magallanes machos y hembras, de acuerdo con estudios diet{\'e}ticos previos de ambas especies. Es probable que estas diferencias act{\'u}en para reducir la competencia entre las 2 especies durante la temporada de reproducci{\'o}n, cuando se ven obligadas a explotar los recursos alrededor de sus colonias. En contraste, los ping{\"u}inos de Magallanes machos y hembras compartieron un nicho de alimentaci{\'o}n similar, medido por las {\'a}reas utilizadas para alimentarse, las profundidades de inmersi{\'o}n, el uso relativo del h{\'a}bitat y los valores tr{\'o}ficos. La falta de segregaci{\'o}n de nichos de alimentaci{\'o}n espec{\'i}ficos por sexo de los ping{\"u}inos de Magallanes en la Isla de los Estados podr{\'i}a estar relacionada con la disponibilidad de alimento en el {\'a}rea y/o el peque{\~n}o tama{\~n}o de la poblaci{\'o}n.},
  langid = {spanish},
  keywords = {Eudyptes chrysocome chrysocome,Isla de los Estados,Niche partitioning,Spheniscus magellanicus,Stable isotopes},
  file = {/Users/tomasmarina/Zotero/storage/3WEX5XNK/Rosciano et al. - 2016 - Do penguins share Evidence of foraging niche segr.pdf}
}

@article{Rosciano2018,
  title = {Foraging Behaviour of {{Magellanic Penguins}} during the Early Chick-Rearing Period at {{Isla}} de Los {{Estados}}, {{Argentina}}},
  author = {Rosciano, Natalia G. and P{\"u}tz, Klemens and Polito, Michael J. and Raya Rey, Andrea},
  year = {2018},
  journal = {Ibis},
  volume = {160},
  number = {2},
  pages = {327--341},
  issn = {1474-919X},
  doi = {10.1111/ibi.12547},
  urldate = {2022-12-13},
  abstract = {Studies of the at-sea distribution and trophic ecology of penguins are essential to understand their role in the broader marine food web. Magellanic Penguins Spheniscus magellanicus have a wide distribution and their foraging behaviour varies across breeding sites and between sexes, among others. In this study, we characterized the at-sea areas, the diving strategies and the relative trophic level of Magellanic Penguins breeding at Isla de los Estados, Argentina, during the early chick-rearing period. In addition, we quantified the interannual, sexual and individual variability in those parameters during three breeding seasons (2011--2013) using devices recording position and dive depth, and obtained blood samples for stable isotope analysis. During the early chick-rearing period, Magellanic Penguins showed small differences between the sexes in their foraging behaviour and large overlap in the at-sea areas used, suggesting no intraspecific variation between sexes. Although there was interannual variability in the foraging behaviour and the trophic level of the penguins, most of the studied nests managed successfully to raise both chicks during the first stage of the breeding cycle (guard stage). The foraging ecology of Magellanic Penguins from this colony was comparable with results of past studies at other breeding colonies. This study contributes to the identification of important at-sea areas for Magellanic Penguins at the southern edge of their distribution and also to the identification of possible threats in the study area such as interaction with fisheries.},
  langid = {english},
  keywords = {at-sea distribution,diving behaviour,Isla de los Estados,Spheniscus magellanicus,stable isotope,trophic ecology},
  file = {/Users/tomasmarina/Zotero/storage/QRGQ92TU/ibi.html}
}

